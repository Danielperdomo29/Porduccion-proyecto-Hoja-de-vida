# Configuración de MongoDB
MONGO_URI=mongodb+srv://tu_usuario:tu_password@cluster.mongodb.net/tu_base_de_datos

# Secreto para sesiones (genera uno aleatorio seguro)
SESSION_SECRET=genera_un_secreto_aleatorio_muy_largo_y_seguro_aqui

# Credenciales de Google OAuth 2.0 (https://console.cloud.google.com/)
GOOGLE_CLIENT_ID=tu_client_id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=tu_client_secret
GOOGLE_CALLBACK_URL=http://localhost:3000/api/auth/google/callback

# URL del frontend (producción)
FRONTEND_URL=https://tu-dominio.com

# Entorno (development o production)
NODE_ENV=development

# Puerto del servidor
PORT=3000
